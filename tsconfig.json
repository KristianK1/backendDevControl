{
	"compilerOptions": {
		"baseUrl": "src",
		"lib": ["dom", "ES2022"],
		"moduleResolution": "node",
		"outDir": "lib",
		"removeComments": false,
		"rootDir": "src",
		"sourceMap": true,
		"target": "ES2022",

		"alwaysStrict": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": false,
		"strictNullChecks": true
	}
}